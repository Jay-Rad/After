{
  "compilerOptions": {
    "noImplicitAny": false,
    "noEmitOnError": true,
    "removeComments": false,
    "sourceMap": true,
    "target": "esnext",
    "module": "es2015",
    "allowJs": false,
    "typeRoots": [ "wwwroot/lib/types/" ]
  },
  "exclude": [
    "node_modules",
    "obj",
    "bin",
    "wwwroot/lib/types/"
  ],
  "compileOnSave": true
}
